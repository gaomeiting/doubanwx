@import "../template/movie-list"
.page-wrap {
	padding: 32rpx;
}
.empty-wrap {
	position: fixed;
	top: 50%;
	left: 50%;
	transform: translate3d(-50%, -80%, 0);
}
.button {
	width: 240rpx;
	height: 70rpx;
	line-height: 70rpx;
	text-align: center;
	color: #fff;
	border-radius: 40rpx;
	background: #40586D;
	margin-top: 32rpx;
}
.img-wrap {
	display: flex;
	flex-wrap: wrap;
}
.img-wrap > view {
	flex: 0 0 218rpx;
	width: 218rpx;
	height: 218rpx;
	margin-right: 16rpx;
	margin-bottom: 16rpx;
	border: 1px solid #f8f8f8;
	box-sizing: border-box;
}
.img-wrap > view:nth-of-type(3n) {
	margin-right: 0;
}
.img-wrap > view:last-of-type {
	background-image: url(data:image/jpeg;base64,);
	background-size: cover;
}
.img-wrap > view image {
	width: 100%;
	height: 100%;
}